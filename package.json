  "author": "leidenglai <leidenglai@foxmail.com>",
  "version": "3.0.1",
  "scripts": {
    "start:front": "cross-env NODE_ENV=development node build/dev.js",
    "dev": "cross-env NODE_ENV=development nodemon --legacy-watch ./server/server.dev.js --watch server --watch build",
    "start": "cd dist && node ./server/server.js",
    "build": "cross-env NODE_ENV=production node build/prod.js && cross-env NODE_ENV=production node build/serv.js",
    "zip": "rm ./dist/my-resume.zip && zip -r ./dist/my-resume.zip ./dist",
    "test": "cross-env NODE_ENV=test mocha 'test/**/*.test.js' --reporter spec -r should -r test/env --timeout 15000 --compilers js:@babel/register"
  },
  "repository": {
